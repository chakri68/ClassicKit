{
  "name": "classickit",
  "version": "0.0.1",
  "description": "Stylized web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/classickit/classickit.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chakri68/classickit.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "watch:build": "stencil build --watch",
    "start": "stencil build --dev --watch --serve --no-open",
    "watch:scss": "node-sass src/styles/classickit.scss www/build/classickit.css -w",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^4.0.0",
    "clsx": "^1.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "@stencil/sass": "^3.0.4",
    "@storybook/addon-essentials": "7.0.24",
    "@storybook/addon-links": "7.0.24",
    "@storybook/addon-styling": "^1.3.2",
    "@storybook/blocks": "7.0.24",
    "@storybook/web-components": "7.0.24",
    "@storybook/web-components-webpack5": "7.0.24",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lit": "2.7.5",
    "node-sass": "^9.0.0",
    "puppeteer": "^20.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.63.6",
    "storybook": "7.0.24"
  },
  "license": "MIT"
}
